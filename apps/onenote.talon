# Microsoft onenote
app: ONENOTE.EXE
-

# editing
create table:
    key(space)
    key(tab)
    key(tab)
    key(ctrl-enter)
    key(tab)

# pages
new page at end:
    key(ctrl-n)

new page below this:
    key(ctrl-alt-n)

new subpage:
    key(ctrl-shift-alt-n)

new window:
    key(ctrl-m)

quick note:
    key(ctrl-shift-m)

go to first page:
    key(alt-pageup)

